<!--空购物车的时候再body上加上empty-->
<body class="empty">
<header class="header container" id="header">
    <ul class="pure-g full-screen-height header-nav">
        <li class="pure-u-1-4">
            <a href="javascript:;" class="go_back_for_cart" onclick="history.go(-1);"></a>
        </li>
        <li class="pure-u-1-2">
            购物车
        </li>
        <li class="pure-u-1-4">
            <a href="javascript:;" class="ft-12 delete_all pull-right row-block">
                <img src="images/icons/delete.png">
            </a>
        </li>
    </ul>
</header>


<div class="shopping-cart-context-area fixed-clear">
    <ul class="cart-list">
        <li class="pure-g">
            <a class="pure-u-1-3">
                <img src="images/goods/goods_1.png">
            </a>

            <div class="pure-u-2-3 box-padding">
                <p class="title">蓝莓</p>

                <p class="goods-size">1.9Kg</p>

                <div class="pure-g padding-top-control">
                    <p class="goods-price pure-u-1-2">￥29.00</p>

                    <div class="number-group pure-u-1-2">
                        <button class="min">-</button>
                        <input type="text" value="1" disabled>
                        <button class="add">+</button>
                    </div>
                </div>
            </div>
            <button class="delete"></button>
        </li>
        <li class="pure-g">
            <a class="pure-u-1-3">
                <img src="images/goods/goods_2.png">
            </a>

            <div class="pure-u-2-3 box-padding">
                <p class="title">福建红心蜜柚—1枚/组 </p>

                <p class="goods-size">1枚/组</p>

                <div class="pure-g padding-top-control">
                    <p class="goods-price pure-u-1-2">￥129.00</p>

                    <div class="number-group pure-u-1-2">
                        <button class="min">-</button>
                        <input type="text" value="1" disabled>
                        <button class="add">+</button>
                    </div>
                </div>
            </div>
            <button class="delete"></button>
        </li>
    </ul>
    <ul class="replace-event">
        <li class="title">
            享受更多优惠-换购活动
        </li>
        <li class="has_allow">
            <a href="replacebuy.html?tag=1">
                <span class="event-tag">促</span><span>活动换购优惠</span>
            </a>
        </li>
        <li class="has_allow">
            <a href="replacebuy.html?tag=2">
                <span class="event-tag">惠</span><span>加价全场购优惠</span>
            </a>
        </li>
        <li class="has_allow">
            <a href="replacebuy.html?tag=3">
                <span class="event-tag">满</span><span>满199换购优惠</span>
            </a>
        </li>
    </ul>
</div>

