/*common*/
.full-screen-height {
    height: 46px;
}

.ft-12 {
    font-size: 12px;
}

.ft-16 {
    font-size: 16px;
}

.ft-20 {
    font-size: 20px;
}

.ft-24 {
    font-size: 24px;
}

.full-width {
    width: 100%;
}

.one-page-screen {
    width: 100%;
    height: 100%;
    overflow: hidden;
}

.row-block {
    overflow: scroll;
    clear: both;
}

i {
    display: inline-block;
    font-style: normal;
    position: relative;
}

body.bg_gray {
    background-color: #F9F9F9;
}

.bg_white {
    background-color: #ffffff;
}

/* Additional formatting for arrow icon */
i.arrow {
    top: 0px;
    left: 1px;
    width: 0.4em;
    height: 0.4em;
    border-right: 0.2em solid #599c3b;
    border-top: 0.2em solid #599c3b;
    transform: rotate(-135deg);
}

.white {
    border-color: #ffffff !important;
}

.area {
    position: relative;
}

.pull-left {
    float: left;
}

.pull-right {
    float: right;
}

.text-right {
    text-align: right;
}

.text-left {
    text-align: left;
}

.text-center {
    text-align: center;
}

.text-hasline {
    text-decoration: line-through;
}

a {
    text-decoration: none;
    color: #000000;
}

.header a {
    color: #ffffff;
}

.header .pure-u-1-4 a, .header .pure-u-1-4 span {
    line-height: 46px;
}

.padding_bottom {
    padding-bottom: 100px;
}

.padding_top_40 {
    padding-top: 40px;
}

.fixed-clear {
    padding-top: 46px;
}

.max-fixed-clear {
    padding-top: 86px;
}

/*common end*/

/*header and footer start*/
.header-nav {
    text-align: center;
    line-height: 46px;
    font-size: 16px;
}

a.go_back_for_cart {
    display: inline-block;
    color: #65b86b;
    height: 30px;
    width: 30px;
    line-height: 30px;
    font-weight: bolder;
    border-radius: 50%;
    text-decoration: none;
    font-size: 20px;
    margin-top: 7px;
    text-align: center;
    float: left;
    margin-left: 10px;
    background: url("/moblieymt/images/icons/icon_allow.png") 7px 5px #ffffff no-repeat;
    background-size: 13px;
}

a.go_back {
    position: fixed;
    top: 10px;
    left: 10px;
    height: 35px;
    width: 35px;
    display: block;
    border-radius: 50%;
    text-align: center;
    line-height: 35px;
    text-decoration: none;
    font-weight: bold;
    background: url("/moblieymt/images/icons/icon_allow.png") 9px -153px #65b86b no-repeat;
    background-size: 13px;
}

.margin-bottom-10 {
    margin-bottom: 10px;
}

.margin-bottom-20 {
    margin-bottom: 20px;
}

#header {
    width: 100%;
    height: 46px;
    background-color: #599c3b;
    color: #fff;
    position: fixed;
    top: 0px;
    z-index: 1999;
    font-size: 14px;
    /* display: none; */
    box-shadow: 3px 3px 15px 2px #365e24;
}

#header .switch {
    width: 100%;
    margin: 9px auto;
    height: 28px;
    background-color: #45802e;
    border-radius: 28px;
    text-align: center;
    line-height: 28px;
    overflow: hidden;
    font-size: 12px;
}

#header .switch a {
    text-decoration: none;
    color: #68a251;
}

#header .switch a.active {
    border-radius: 28px;
    background-color: #ffffff;
    color: #508630;
}

#header .pure-u-1-4 button.location {
    margin: 8px 0px 0px 10px;
    height: 26px;
    width: 26px;
    outline: none;
    border: none;
    background: url("/moblieymt/images/icons/icon-location.png") center center no-repeat;
    background-size: cover;
    vertical-align: -5px;
}

#header .search {
    display: block;
    width: 26px;
    height: 26px;
    background: url("/moblieymt/images/icons/icon-search.png") center right no-repeat;
    background-size: cover;
    float: right;
    outline: none;
    border: none;
    margin: 9px 15px 0px 0px;
}

a.cart_btn {
    position: relative;
    display: inline-block;
    height: 45px;
    margin-right: 15px;
}

.cart_btn .shopping-cart-num {
    display: inline-block;
    height: 16px;
    width: 16px;
    border-radius: 50%;
    background-color: #c9302c;
    position: absolute;
    z-index: 99999;
    color: #fff;
    vertical-align: middle;
    font-size: 12px;
    line-height: 16px;
    text-align: center;
}

#header .cart_btn img {
    height: 25px;
    width: 25px;
}

.footer-nav .cart_btn .shopping-cart-num {
    top: -5px;
    right: 12px;
}

#header .cart_btn .shopping-cart-num {
    top: 6px;
    right: -9px;
}

.shopping-cart-line .cart_btn .shopping-cart-num {
    top: 0px;
    right: -9px;
}

.footer-nav {
    position: fixed;
    bottom: -1px;
    height: 44px;
    width: 100%;
    background-color: rgba(240, 240, 240, 0.95);
    padding-top: 7px;
    z-index: 9999;
}

.footer-nav ul {
    display: block;
    /* overflow: hidden; */
    font-size: 12px;
}

.footer-nav ul li {
    display: block;
    float: left;
    width: 20%;
    text-align: center;
}

.footer-nav ul li a {
    display: block;
    height: auto;
    margin: 0 auto;
    color: #656565;
    text-decoration: none;
}

.footer-nav a img {
    display: block;
    width: 30%;
    margin: 0 auto 3px auto;
}

.banner-area .bxslider {
    width: 100%;
}

.banner-area .bxslider img.first {
    display: block;
}

.banner-area .bxslider img {
    width: 100%;
    display: none;
}

.loadmore img.loadmore_image {
    padding: 0px 6px 0px 0px;
    width: 20px;
    height: 20px;
    vertical-align: text-bottom;
}

.prompt {
    color: #666666;
    width: 100%;
    height: 20px;
    padding: 5px 0px 20px 0px;
    text-align: center;
    line-height: 20px;
    vertical-align: middle;
}

/*header and footer start*/